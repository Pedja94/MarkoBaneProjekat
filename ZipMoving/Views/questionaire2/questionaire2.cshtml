@model ZipMoving.Models.questionaire2Model
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Zip Moving - Quote</title>
    <!-- Bootstrap DATEPICKER -->
    <link href="~/Content/vendor/datepicker/css/bootstrap-datepicker3.css" rel="stylesheet">


    <!-- Bootstrap Core CSS -->
    <link href="~/Content/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="~/Content/vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Merriweather:400,300,300italic,400italic,700,700italic,900,900italic' rel='stylesheet' type='text/css'>

    <!-- Plugin CSS -->
    <link href="~/Content/vendor/magnific-popup/magnific-popup.css" rel="stylesheet">

    <!-- Theme CSS -->
    <link href="~/Content/css/creative.min.css" rel="stylesheet">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- FURNITURE ICONS FONT CSS -->
    <link href="~/Content/css/icons 2 font/font/flaticon.css" rel="stylesheet" type="text/css">

</head>

<body id="page-top">

    <nav id="mainNav" class="navbar navbar-default navbar-fixed-top" style="height:70px;">
        <div class="container-fluid" style="height:70px;">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header" style="padding-top:10px;">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span> Menu <i class="fa fa-bars"></i>
                </button>
                <a class="navbar-brand page-scroll" href="#page-top" style="font-size:large;">ZIP Moving</a>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1" style="padding-top:10px;">
                <ul class="nav navbar-nav navbar-right">

                    <li>
                        <a class="page-scroll" href="@Url.Action("Index", "Index")" style="font-size:medium;"><span class="glyphicon glyphicon-arrow-left"></span> &nbsp Home</a>
                    </li>
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container-fluid -->
    </nav>


    <!-- SURVEY PART 1 START -->
    <header style="background-image:none; background-color:#838383;" id="homeHeading1">
        <div class="header-content">
            <div class="header-content-inner">
                <h1 id="homeHeading">Let's start with your quote</h1>
                <p style="font-size:x-large;">In this survey we will determine requirements for your move.</p><hr>
                <p>Survey can be compited in less then 15min,no obligation, no phone calls, no spam emails, find about your price online if you use our online inventory. We do not shere your information with the trth party.</p>
                <a href="#about" class="btn btn-primary btn-xl page-scroll">Go</a>
            </div>
        </div>
    </header>
    <!-- SURVEY PART 1 END -->
    <!-- SURVEY PART 2 START -->
    <form method="post" action="@Url.Action("SubmitEverything", "questionaire2")">


        <section class="bg-primary" id="about" style="font-family:'Open Sans','Helvetica Neue',Arial,sans-serif;">
            <div class="container">
                <div class="row">
                    <div class="col-lg-8 col-lg-offset-2 text-center">
                        <div class="row">

                            <!-- FULL NAME START -->
                            <div class="col-md-8 col-md-offset-2 ">
                                <h3>Your full name?</h3>
                                @Html.TextBoxFor(m => m.FullName, new { @class = "form-control", @id = "fullname", @name = "fullname", @placeholder = "Full name", })
                                @Html.ValidationMessageFor(model => model.FullName, "", new { @class = "text-danger" })
                            </div>
                        </div>
                        <hr>

                        <!-- FULL NAME END -->
                        <!-- TYPE OF MOVE START -->
                        <div class="row">
                            <div class="col-md-12">
                                <h3>
                                    What type of move is this?
                                </h3>
                                <hr>

                                <div class="form-group">
                                    <div class="radio" style="font-size:medium; text-align:center;">
                                        <label class="onHoverOurs">
                                            @Html.RadioButtonFor(m => m.TypeOfMove, "In State - Local Move (Charged by Hourly Rate + Additional Services)") In State - Local Move (Charged by Hourly Rate + Additional Services)                    
                                        </label><br><br>
                                        <label class="onHoverOurs">
                                            @Html.RadioButtonFor(m => m.TypeOfMove, "Out Of the State - Interstate Move (Charged by Weight or Cuft)") Out Of the State - Interstate Move (Charged by Weight or Cuft)                                           
                                        </label>
                                    </div>
                                    @Html.ValidationMessageFor(model => model.TypeOfMove, "", new { @class = "text-danger" })
                                </div>
                            </div>
                        </div>
                        <hr>
                        <hr>
                        <!-- TYPE OF MOVE END -->
                        <a href="#services" class="page-scroll btn btn-default btn-xl sr-button">Next: Pickup Information</a>
                    </div>
                </div>
            </div>
        </section>
        <!-- SURVEY PART 2 END -->
        <!-- SURVEY PART 3 START -->
        <section id="services">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 text-center">
                        <h2 class="section-heading">Pickup Information</h2>
                        <hr class="primary">
                    </div>
                </div>
            </div>
            <div class="container">
                <!-- 1st row start -->
                <div class="row">

                    <div class="col-md-12 text-center">
                        <h3 style="font-weight:bold;">What do you want to move?</h3>
                    </div>

                    <div class="col-md-12 text-center" style="font-size:medium;">
                        <div class="form-inline">
                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.ToMove, "Everything") Everything                                    
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.ToMove, "Just A Few Things") Just A Few Things                                    
                                </label>
                            </div>
                        </div>
                        @Html.ValidationMessageFor(model => model.ToMove, "", new { @class = "text-danger" })
                    </div>
                </div>
                <!-- 1st row end -->
                <br><br><br>
                <!-- 2nd row start -->
                <div class="row">

                    <div class="col-md-12 text-center">
                        <h3 style="font-weight:bold;">Who is going to be present at the pickup location?</h3>
                    </div>

                    <div class="col-md-12 text-center" style="font-size:medium;">
                        <div class="form-inline">
                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.PresentAtPickup, "My self") My self 
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.PresentAtPickup, "Someone Else") Someone Else               
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;

                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.PresentAtPickup, "I'm Not Sure") I'm Not Sure  
                                </label>
                            </div>
                        </div>
                        @Html.ValidationMessageFor(model => model.PresentAtPickup, "", new { @class = "text-danger" })
                    </div>
                </div>
                <!-- 2nd row end -->
                <br><br><br>
                <!-- 3rd row start -->
                <div class="row">

                    <div class="col-md-12 text-center">
                        <h3 style="font-weight:bold;">What is the ZIP code of the pickup location?</h3>
                    </div>

                    <div class="col-md-4 col-md-offset-4 text-center" style="font-size:medium;">
                        @Html.TextBoxFor(m => m.ZIPPickup, new { @class = "form-control", @id = "zipINPUT", @name = "zipINPUT", @placeholder = "ZIP CODE", @style = "text-align:center;", })
                        @Html.ValidationMessageFor(model => model.ZIPPickup, "", new { @class = "text-danger" })
                    </div>
                </div>
                <!-- 3rd row end -->
                <br><br><br>
                <!-- 4th row start -->
                <div class="row">

                    <div class="col-md-12 text-center">
                        <h3 style="font-weight:bold;">Do you have additional stop off at the pickup location?</h3>
                    </div>

                    <div class="col-md-12 text-center" style="font-size:medium;">
                        <div class="form-inline">
                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.AdditionalStopOffAtPickup, "Yes") Yes         
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.AdditionalStopOffAtPickup, "No") No          
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;

                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.AdditionalStopOffAtPickup, "Not Sure") Not Sure          
                                </label>
                            </div>
                        </div>
                        @Html.ValidationMessageFor(model => model.AdditionalStopOffAtPickup, "", new { @class = "text-danger" })
                    </div>
                </div>
                <!-- 4th row end -->
                <br><br><br>

                <!-- 5th row start -->
                <div class="row">

                    <div class="col-md-12 text-center">
                        <h3 style="font-weight:bold;">Where are you moving from?</h3>
                    </div>

                    <div class="col-md-12 text-center" style="font-size:medium;">
                        <div class="form-inline">
                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.MovingFrom, "House") House                  
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.MovingFrom, "Townhouse") Townhouse                   
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;

                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.MovingFrom, "Apartment") Apartment                
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;

                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.MovingFrom, "Office") Office                       
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;

                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.MovingFrom, "Storage") Storage               
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                        </div>
                        @Html.ValidationMessageFor(model => model.MovingFrom, "", new { @class = "text-danger" })
                    </div>
                </div>
                <!-- 5th row end -->
                <br><br><br>
                <!--6th row start -->
                <div class="row">

                    <div class="col-md-12 text-center">
                        <h3 style="font-weight:bold;">What is the size of your office storage?</h3>
                    </div>

                    <div class="col-md-12 text-center" style="font-size:medium;">
                        <div class="form-inline">
                            @Html.DropDownListFor(m => m.SizeOfOfficePickup, Model.GetAllSizes(), new { @size = "1", @class = "form-control" })
                        </div>
                        @Html.ValidationMessageFor(model => model.SizeOfOfficePickup, "", new { @class = "text-danger" })
                    </div>
                </div>
                <!-- 6th row end -->
                <br><br><br>

                <!-- 7th row start -->
                <div class="row">

                    <div class="col-md-12 text-center">
                        <h3 style="font-weight:bold;">
                            Does your apartment building/office regulate COI?
                        </h3>
                        <p style="font-size:small; color:gray;">(Certificate of Insurance)</p>
                    </div>

                    <div class="col-md-12 text-center" style="font-size:medium;">
                        <div class="form-inline">
                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.COIPickup, "Yes") Yes                    
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.COIPickup, "No") No                       
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;

                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.COIPickup, "I'm Not Sure") I'm Not Sure                   
                                </label>
                            </div>
                        </div>
                        @Html.ValidationMessageFor(model => model.COIPickup, "", new { @class = "text-danger" })
                    </div>
                </div>
                <!-- 7th row end -->
                <br><br><br>
                <!-- 8th row start -->
                <div class="row">

                    <div class="col-md-12 text-center">
                        <h3 style="font-weight:bold;">Are movers required to use elevator at the pickup location?</h3>
                    </div>

                    <div class="col-md-12 text-center" style="font-size:medium;">
                        <div class="form-inline">
                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.ElevatorPickup, "Yes") Yes                        
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.ElevatorPickup, "No") No                     
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;

                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.ElevatorPickup, "I'm Not Sure") I'm Not Sure                        
                                </label>
                            </div>
                        </div>
                        @Html.ValidationMessageFor(model => model.ElevatorPickup, "", new { @class = "text-danger" })
                    </div>
                </div>
                <!-- 8th row end -->
                <br><br><br>
                <!-- 9th row start -->
                <div class="row">

                    <div class="col-md-12 text-center">
                        <h3 style="font-weight:bold;">Are movers required to use any stairs (Besides the stairs inside of the House/Apartment/Office)?</h3>
                    </div>

                    <div class="col-md-12 text-center" style="font-size:medium;">
                        <div class="form-inline">
                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.StairsPickup, "No, only stairs inside of the House/Apartments") No, only stairs inside of the House/Apartments                
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.StairsPickup, "Yes, more than 10 steps") Yes, more than 10 steps                       
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;

                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.StairsPickup, "Yes, more than 20 steps") Yes, more than 20 steps                   
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;

                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.StairsPickup, "I'm Not Sure") I'm Not Sure                  
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                        </div>
                        @Html.ValidationMessageFor(model => model.StairsPickup, "", new { @class = "text-danger" })
                    </div>
                </div>
                <!-- 9th row end -->
                <br><br><br>
                <!-- 10th row start -->
                <div class="row">

                    <div class="col-md-12 text-center">
                        <h3 style="font-weight:bold;">How is the parking on the pickup location?</h3>
                    </div>

                    <div class="col-md-12 text-center" style="font-size:medium;">
                        <div class="form-inline">
                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.ParkingPickup, "Private Driveway") Private Driveway                   
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.ParkingPickup, "Street Parking in Front (Less than 75ft)") Street Parking in Front (Less than 75ft)
                                    @*<input type="radio" name="inlineRadioOptions" id="inlineRadio2" value="option1"><span> Street Parking in Front (Less than 75ft) </span>*@
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;

                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.ParkingPickup, "Street Parking Further Away (more than 75ft)") Street Parking Further Away (more than 75ft)
                                    @*<input type="radio" name="inlineRadioOptions" id="inlineRadio2" value="option1"><span> Street Parking Further Away (more than 75ft) </span>*@
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                            <br>
                            @Html.ValidationMessageFor(model => model.ParkingPickup, "", new { @class = "text-danger" })
                            <br><br><br><br>

                            <a href="#about" class="page-scroll btn btn-primary btn-xl sr-button">Previous: Basic Information</a> &nbsp;
                            <a href="#deliveryINFO" class="btn btn-primary btn-xl page-scroll">Next: Delivery Information</a>



                        </div>
                    </div>
                </div>
                <!-- 10th row end -->

            </div>
        </section>
        <!-- SURVEY PART 3 END -->
        <!-- SURVEY PART 4 START -->
        <section id="deliveryINFO" style="background-color:#F05F40; color:white;">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 text-center">
                        <h2 class="section-heading">Delivery Information</h2>
                        <hr class="primary" style="border-color:white;">
                    </div>
                </div>
            </div>
            <div class="container">
                <!-- 1st row start -->
                <div class="row">

                    <div class="col-md-12 text-center">
                        <h3 style="font-weight:bold;">What is the ZIP code of your new residence?</h3>
                    </div>

                    <div class="col-md-12 text-center" style="font-size:medium;">
                        <div class="col-md-4 col-md-offset-4 text-center" style="font-size:medium;">
                            @Html.TextBoxFor(m => m.ZIPDelivery, new { @class = "form-control", @id = "zipINPUT1", @name = "zipINPUT1", @placeholder = "ZIP CODE", @style = "text-align:center;", })
                            @Html.ValidationMessageFor(model => model.ZIPDelivery, "", new { @class = "text-danger" })
                            @*<input type="text" class="form-control" id="zipINPUT" placeholder="ZIP CODE" style="text-align:center;">*@
                        </div>
                    </div>
                </div>
                <!-- 1st row end -->
                <br><br><br>
                <!-- 2nd row start -->
                <div class="row">

                    <div class="col-md-12 text-center">
                        <h3 style="font-weight:bold;">Where are you moving to?</h3>
                    </div>

                    <div class="col-md-12 text-center" style="font-size:medium;">
                        @*ovde je stajao form tag umesto div (dole)*@
                        <div class="form-inline">
                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.MovingTo, "House") House
                                    @*<input type="radio" name="inlineRadioOptions" id="inlineRadio1" value="option1"><span> House </span>*@
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.MovingTo, "Townhouse") Townhouse
                                    @*<input type="radio" name="inlineRadioOptions" id="inlineRadio2" value="option1"><span> Townhouse </span>*@
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;

                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.MovingTo, "Apartment") Apartment
                                    @*<input type="radio" name="inlineRadioOptions" id="inlineRadio2" value="option1"><span> Apartment </span>*@
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;

                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.MovingTo, "Office") Office
                                    @*<input type="radio" name="inlineRadioOptions" id="inlineRadio2" value="option1"><span> Office </span>*@
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;

                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.MovingTo, "Public Storage") Public Storage
                                    @*<input type="radio" name="inlineRadioOptions" id="inlineRadio2" value="option1"><span> Public Storage </span>*@
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                        </div>
                        @Html.ValidationMessageFor(model => model.MovingTo, "", new { @class = "text-danger" })
                    </div>
                </div>
                <!-- 2nd row end -->
                <br><br><br>
                <!-- 3rd row start -->
                <div class="row">

                    <div class="col-md-12 text-center">
                        <h3 style="font-weight:bold;">
                            Does your apartment building/office regulate COI?
                        </h3>
                        <p style="font-size:small; color:#FFF;">(Certificate of Insurance)</p>
                    </div>

                    <div class="col-md-12 text-center" style="font-size:medium;">
                        @*ovde je stajao form tag umesto div (dole)*@
                        <div class="form-inline">
                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.COIDelivery, "Yes") Yes
                                    @*<input type="radio" name="inlineRadioOptions" id="inlineRadio1" value="option1"><span> Yes </span>*@
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.COIDelivery, "No") No
                                    @*<input type="radio" name="inlineRadioOptions" id="inlineRadio2" value="option1"><span> No </span>*@
                                </label>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;

                            <div class="form-group radio">
                                <label class="onHoverOurs">
                                    @Html.RadioButtonFor(m => m.COIDelivery, "I'm Not Sure") I'm Not Sure
                                    @*<input type="radio" name="inlineRadioOptions" id="inlineRadio2" value="option1"><span> I'm Not Sure </span>*@
                                </label>
                            </div>
                        </div>
                        @Html.ValidationMessageFor(model => model.COIDelivery, "", new { @class = "text-danger" })
                    </div>
                </div>
                <!-- 3rd row end -->
                <br><br><br>
                <!-- 4th row start -->
                <div class="row">

                    <div class="col-md-12 text-center">
                        <h3 style="font-weight:bold;">Are movers required to use elevator at the delivery location?</h3>
                    </div>

                    <div class="col-md-12 text-center" style="font-size:medium;">
                        @*ovde je stajao form tag umesto div (dole)*@
                        <div class="form-inline">
